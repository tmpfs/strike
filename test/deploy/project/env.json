{
	"name": "project",
	"data": {
		"array": [ "a", "b", "c", "d e f" ],
		"object": { "name": "$project_name", "version": "$project_version" }
	},
	"env": {
		"home": "$HOME",
		"home_escaped": "\$HOME",		
		"user": "$USER",
		"path": "$PATH",		
		"shell": "$SHELL",
		"unset": "${UNSET_ENV_VARIABLE:-}",
		"foo": "bar",
		"project-name": "${project_name}",
		"project-version": "${project_version}",
		"bad.name~format": "bar",
		"backtick": "`bar`",
		"rmdir": "rmdir target",
		"rmdir-backtick": "`rmdir target`",
		"and": "a && b",
		"or": "c || d",	
		"4": "2 + 2",
		"bgjob": "ls -la &",		
		"regex": "[[ \"abc\" =~ a ]]",
		"backslash": "a\bc",
		"newline": "first\nsecond",
		"history": "!history",
		"shebang": "#!/usr/bin/env bash -x",
		"archive": {
			"name": "${project_name}-${project_version}.tgz",
			"dir": "${target}",
			"path": "${target}/${project_name}-${project_version}.tgz"
		}
	}
}